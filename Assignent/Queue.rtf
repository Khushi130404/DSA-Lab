2.WRITE A PROGRAM TO PERFORM INSERT , DELETE AND PEEK OPERATION OF A CIRCULAR QUEUE.\par
\i0\fs28 #include <stdio.h>\par
\par
class queue\par
\{\par
    int f=-1,r=-1;\par
    int a[5];\par
    public: \par
\par
    int isfull()\par
    \{\par
        if(r==4 && f==0)\par
        \{\par
            return 1;\par
        \}\par
        else if(r==f-1)\par
        \{\par
            return 1;\par
        \}\par
        else\par
        \{\par
            return 0;\par
        \}\par
    \par
    \}\par
    int isempty()\par
    \{\par
        if(r==-1 && f==-1)\par
        \{\par
            return 1;\par
        \}\par
        else if(r==f-1)\par
        \{\par
            r=-1;\par
            f=-1;\par
            return 1;\par
        \}\par
        else\par
        \{\par
            return 0;\par
        \}\par
    \}\par
    void insert()\par
    \{\par
        if(isfull())\par
        \{\par
            printf("\\nQueue is full\\n");\par
        \}\par
        else\par
        \{\par
            if(r==-1 && f==-1)\par
            \{\par
                r++;\par
                f++;\par
            \}\par
            else if(r==4)\par
            \{\par
                r=0;\par
            \}\par
            else\par
            \{\par
                r++;\par
            \}\par
            printf("Enter the number: ");\par
            scanf("%d",&a[r]);\par
        \}   \par
    \}\par
    void delet()\par
    \{\par
        if(isempty())\par
        \{\par
            printf("\\nQueue is empty\\n");\par
        \}\par
        else\par
        \{\par
\par
            if(f==4 && r==4)\par
            \{\par
                f=-1;\par
                r=-1;\par
                printf("\\n%d",a[4]);\par
            \}\par
            if(f==4)\par
            \{\par
                f=-1;\par
                printf("\\n%d",a[4]);\par
            \}\par
            else\par
            \{\par
                f++;\par
                printf("\\n%d",a[f-1]);\par
            \}\par
        \}\par
            \par
    \}\par
    void peek()\par
    \{\par
        if(!isempty())\par
        \{\par
            printf("\\n%d",a[r]);\par
        \}\par
    \}\par
\par
\};\par
int main()\par
\{\par
    queue q;\par
    int n=0;\par
    while(n!=4)\par
    \{\par
        printf("\\nEnter 1 for insert\\nEnter 2 for delete\\nEnter 3 for peek\\nEnter 4 for exit\\n");\par
        scanf("%d",&n);\par
        switch(n)\par
        \{\par
            case 1: q.insert();\par
                    break;\par
            case 2: q.delet();\par
                    break;\par
            case 3: q.peek();\par
                    break;\par
        \}\par
    \}\par
    return 0;\par
\}\i\fs40\par
