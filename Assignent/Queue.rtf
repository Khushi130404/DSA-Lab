\pard\sa200\sl276\slmult1\b\i\f1\fs40 1. WRITE A PROGRAM TO PERFORM INSERT , DELETE AND PEEK OPERATION OF A SIMPLE QUEUE.\par
\i0\fs28 //Simple Queue\par
\par
#include <stdio.h>\par
#define SIZE 10\par
class queue\par
\{\par
    int front=-1;\par
    int rear=-1;\par
    int arr[SIZE];\par
    public:\par
\par
    int isempty()\par
    \{\par
        if(front==rear)\par
        \{\par
            return 1;\par
        \}\par
        else\par
        \{\par
            return 0;\par
        \}\par
    \}\par
    int isfull()\par
    \{\par
        if(rear==SIZE-1)\par
        \{\par
            return 1;\par
        \}\par
        else\par
        \{\par
            return 0;\par
        \}\par
    \}\par
    void insert()\par
    \{\par
        if(isfull())\par
        \{\par
            printf("\\nQueue overflow.....");\par
        \}\par
        else\par
        \{\par
            rear++;\par
            printf("\\nEnter the number to insert: ");\par
            scanf("%d",&arr[rear]);\par
        \}\par
    \}\par
    void delet()\par
    \{\par
        if(isempty())\par
        \{\par
            printf("\\nQueue underflow.....");\par
        \}\par
        else\par
        \{\par
            front++;\par
            printf("\\narr[%d]= %d",front,arr[front]);\par
        \}\par
    \}\par
    void peek()\par
    \{\par
        if(isempty())\par
        \{\par
            printf("\\nQueue underflow.....");\par
        \}\par
        else\par
        \{\par
            printf("\\narr[%d]= %d",front+1,arr[front+1]);\par
        \}\par
    \}\par
\};\par
\par
int main()\par
\{\par
    int n=0;\par
    queue q;\par
    while(n!=4)\par
    \{\par
        printf("\\n\\nEnter choice:-\\n1. Insert\\n2. Delete\\n3. Peek\\n4. Exit\\n");\par
        scanf("%d",&n);\par
        switch(n)\par
        \{\par
            case 1: q.insert();\par
                    break;\par
            case 2: q.delet();\par
                    break;\par
            case 3: q.peek();\par
                    break;\par
            case 4: break;\par
        \}\par
    \}\par
    return 0;\par
\}\i\fs40\par
